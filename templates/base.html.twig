				<!-- Content Wrapper. Contains page content -->
				<div
					class="content-wrapper">
					<!-- Content Header (Page header) -->
					<div class="content-header">
					<div class="container">
	{% for label, flashes in app.session.flashbag.all %}
		{% for flash in flashes %}
			<div class="alert alert-{{ label }} alert-dismissible fade show" role="alert">
				<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
				<strong>
					<!--i class="icon fas fa-ban"></i-->
					{{ label|title }}
					!</strong>
				{{ flash }}
			</div>
		{% endfor %}
	{% endfor %}
</div>
						<div class="container">
							<div class="row mb-2">
								<div class="col-sm-8">
									<h1 class="m-0">Bin<span class="text-danger">C</span>ard Manager
										{% if app.user %}
											{% if app.user.siteActif %}
												<span class="fa fa-angle-double-right"></span>
												{{app.user.siteActif.designation }}
												{% if app.user.siteActif.isWarehouse %}
													<span title="ce site est un warehouse" class="fa fa-tags"></span>
												{% endif %}
											{% endif %}
										{% endif %}
										{% block lesite %}
											<span class="fa fa-angle-double-right"></span>
											Gestion de stocks
										{% endblock %}
									</h1>
								</div>
								<!-- /.col -->
								<div class="col-sm-4">
									<div class="btn-group float-sm-right">
										{% block lesboutons %}
											<a title="Retour à la liste " class="btn btn-info">
												<i class="fa fa-reply-all"></i>
											</a>
											<a type="button" title="Ajouter nouveau " class="btn btn-info">
												<i class="fas fa-plus-circle"></i>
											</a>
											<a type="button" title="Modifier élément  " class="btn btn-info">
												<i class="fas fa-edit"></i>
											</a>
											<a type="button" title="Supprimer élément" class="btn btn-info">
												<i class="fas fa-trash"></i>
											</a>
										{% endblock %}
										<a type="button" title="Imprimer la page " onclick="window.print()" class="btn btn-info">
											<i class="fas fa-print"></i>
										</a>
									</div>
								</div>
								<!-- /.col -->
							</div>
							<!-- /.row -->
						</div>
						<!-- /.container-fluid -->
					</div>
					<!-- /.content-header -->

					<!-- Main content -->
					<div class="content">
						<div class="container">
							<div class="row">
								<div class="col-lg-12">
									<div class="card card-primary card ">
										<div class="card-header">
											<h1 class="card-title">
												{% block titre %}
													Liste des sorties
												{% endblock %}
											</h1>
										</div>
										<!-- /.card-header -->
										<div class="card-body"> {% block body %}{% endblock %}
											</div>
										</div>
										<!-- /.card -->
									</div>
									<!-- /.col-md-6 -->
								</div>
								<!-- /.row -->
							</div>
							<!-- /.container-fluid -->
						</div>
						<!-- /.content -->
